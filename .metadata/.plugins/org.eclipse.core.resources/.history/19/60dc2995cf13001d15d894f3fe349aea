package jungol;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String str = sc.next();
		
		for(int i = 0; i < str.length(); i++) {
			for(int j = 0; j < str.length(); j++) {
				System.out.print(str.charAt(j));
			}
			System.out.println();
		}
	}
}