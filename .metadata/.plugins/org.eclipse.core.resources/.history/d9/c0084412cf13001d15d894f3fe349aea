package jungol;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int h = sc.nextInt();
		int w = sc.nextInt();
		int count = 1;
		sc.close();
		
		for(int i = 1; i <= h; i++) {
			for(int j = 1; j <= w; j++) {
				System.out.print(count + " ");
				count++;
			}
			System.out.println();
		}
	}
}