package jungol;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String str = sc.next();
		
		char[] chArr = new char[str.length()];
		
		for(int i = 0; i < chArr.length; i++) {
			System.out.println(chArr[i]);
		}
	}
}