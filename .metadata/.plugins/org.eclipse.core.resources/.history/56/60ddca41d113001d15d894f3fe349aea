package jungol;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String str = sc.next();
		char str1 = str.charAt(0);
		char str2 = str.charAt(str.length() - 1);
		char ch;
		
		for(int i = 0; i < str.length(); i++) {

				ch = str.charAt(0);
				str1 = str.charAt(1);
				str2 = str1;
				System.out.println(str);

		}
	}
}